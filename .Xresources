xterm*locale: true
xterm*saveLine: 4096
xterm*faceName: Liberation Mono:size=10:antialias=false
xterm*font: 7x13

! Base16 Chalk
! Scheme: Chris Kempson (http://chriskempson.com)

#define base00 #1C2023 # Black
#define base01 #C7AE95 # Red
#define base02 #95C7AE # Green
#define base03 #AEC795 # Yellow
#define base04 #AE95C7 # Blue
#define base05 #C795AE # Magenta
#define base06 #95AEC7 # Cyan
#define base07 #C7CCD1 # White
#define base08 #747C84 # Bright Black
#define base09 $base01 # Bright Red
#define base10 $base02 # Bright Green
#define base11 $base03 # Bright Yellow
#define base12 $base04 # Bright Blue
#define base13 $base05 # Bright Magenta
#define base14 $base06 # Bright Cyan
#define base15 #F3F4F5 # Bright White
#define base16 #C7C795
#define base17 #C79595
#define base18 #393F45
#define base19 #565E65
#define base20 #ADB3BA
#define base21 #DFE2E5

*.foreground:      base05
*.background:      base01
*.cursorColor:     base05

*.color0:          base00
*.color1:          base08
*.color2:          base0B
*.color3:          base0A
*.color4:          base0D
*.color5:          base0E
*.color6:          base0C
*.color7:          base05

*.color8:          base03
*.color9:          base08
*.color10:         base0B
*.color11:         base0A
*.color12:         base0D
*.color13:         base0E
*.color14:         base0C
*.color15:         base07

! Note: colors beyond 15 might not be loaded (e.g., xterm, urxvt),
! use 'shell' template to set these if necessary
*.color16:         base09
*.color17:         base0F
*.color18:         base01
*.color19:         base02
*.color20:         base04
*.color21:         base06
